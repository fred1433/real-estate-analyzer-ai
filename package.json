{
  "name": "real-estate-analyzer-ai",
  "version": "1.0.0",
  "description": "Analyseur immobilier IA - Prototype impressionnant",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "deploy:prepare": "npm run build && cp -r frontend/dist backend/public"
  },
  "keywords": ["real-estate", "ai", "openai", "analysis"],
  "author": "Real Estate Analyzer AI",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 